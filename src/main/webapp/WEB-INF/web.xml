<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

  <servlet>
    <servlet-name>SignInServlet</servlet-name>
    <servlet-class>org.mycompany.myname.controller.SignInServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>NotesServlet</servlet-name>
    <servlet-class>org.mycompany.myname.controller.NotesServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>LoadServlet</servlet-name>
    <servlet-class>org.mycompany.myname.controller.LoadServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>EditText</servlet-name>
    <servlet-class>org.mycompany.myname.controller.EditText</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>SignInServlet</servlet-name>
    <url-pattern>/signIn</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>NotesServlet</servlet-name>
    <url-pattern>/noteServlet</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>LoadServlet</servlet-name>
    <url-pattern>/servletInNotes</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>EditText</servlet-name>
    <url-pattern>/editTextServlet</url-pattern>
  </servlet-mapping>

</web-app>